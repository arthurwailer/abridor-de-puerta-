AT+CPIN? // COMPROBAR SI EL SIM ESTA LISTO PARA SER USADO
+CPIN: READY // RETORNO 

AT+COPS? // COMPRUEBA SI SE CONECTA A UNA RED 
+COPS: 0,0,"CL ENTEL PCS // RETORNO 

AT+CCALR? //COMPRUEBA SI EL MODULO Y SIM ESTAN HABILITADOS PARA HACER LLAMADAS 
+CCLAR:1 // RETORNO 1 Y ESTA OK PARA HACER LLAMADAS 

AT+GSMBUSY=0 // COMPRUEBA SI ESTA BLOQUIADO EL MODULO SIM PARA HACER LLAMADAS 

ATD+56978924091; // PARA HACER LLAMADAS SE ANOTA EL NUMERO COMPLETO INCLUYENDO EL "+" Y ";" AL FINAL.
ATD; // COLGAR SE AGREGA ";" 

